<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="employeeConstructorBean" class="com.aoher.model.Employee">
        <constructor-arg value="1" />
        <constructor-arg value="first" />
    </bean>

    <bean id="employeeGetterBean" class="com.aoher.model.Employee">
        <property name="employeeId" value="2" />
        <property name="employeeName" value="second" />
    </bean>

</beans>